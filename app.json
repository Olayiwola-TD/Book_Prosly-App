{
  "expo": {
    "name": "BookApp",
    "slug": "BookApp",
    "version": "1.5.0",
    "orientation": "portrait",
    "scheme": "com.eBookAudioBookApp.eBookAudioBookApp",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationWhenInUseUsageDescription": "BookApp use Location for select places",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "BookApp use Location for select places",
        "NSLocationAlwaysUsageDescription": "BookApp use Location for select places"
      },
      "usesAppleSignIn": false,
      "bundleIdentifier": "com.eBookAudioBookApp.eBookAudioBookApp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.eBookAudioBookApp.eBookAudioBookApp",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "USER_FACING_NOTIFICATIONS",
        "LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_CONTACTS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "useNextNotificationsApi": true,
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC1PD_A1j--Aw4F0iRkC5KZsoxexw6mpnI"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "443eee11-f129-4583-98a6-e59a38d9367d"
      }
    }
  },
  "name": "BookApp"
